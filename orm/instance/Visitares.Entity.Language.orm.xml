<doctrine-mapping xmlns='http://doctrine-project.org/schemas/orm/doctrine-mapping' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd'>
	<entity name='Visitares\Entity\Language' table='language'>
		<!-- Meta -->
		<id name='id' type='integer'>
			<generator strategy='AUTO' />
		</id>
		<field name='creationDate' type='datetime' />
		<field name='modificationDate' type='datetime' nullable='true' />

		<!-- Data -->
		<field name='isDefault' type='boolean' />
		<field name='code' type='string' length='2' />
		<field name='label' type='string' length='250' />
	</entity>
</doctrine-mapping>