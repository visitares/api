<doctrine-mapping xmlns='http://doctrine-project.org/schemas/orm/doctrine-mapping' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd'>
	<entity name='Visitares\Entity\ImageGroup' table='imagegroup'>
		<!-- Meta -->
		<id name='id' type='integer'>
			<generator strategy='AUTO' />
		</id>
		<field name='creationDate' type='datetime' />
		<field name='modificationDate' type='datetime' />

		<!-- Data -->
		<field name='type' type='integer' />
		<field name='label' type='string' length='255' />
		<field name='instances' type='string' nullable='true' />
	</entity>
</doctrine-mapping>